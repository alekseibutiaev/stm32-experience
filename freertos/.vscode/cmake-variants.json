{
  "buildType": {
    "default": "Debug freertos",
    "description": "build variants",
    "choices": {
      "Debug freertos": {
        "short": "Debug freertos",
        "buildType": "Debug",
        "settings" : {
          "CMAKE_TOOLCHAIN_FILE": "tools_descriptor.cmake",
          "CMAKE_VERBOSE_MAKEFILE" : "ON",
          "FreeRTOS_VERSION": "V10.6.2",
          "FREERTOS_CONFIG_FILE_DIRECTORY" : "${workspaceFolder}/freertos",
          "FREERTOS_PORT" : "GCC_ARM_CM3",
          "DUMP_ASM": "ON"
        } 
      },
      "Release freertos": {
        "short": "Release freertos",
        "buildType": "Release",
        "settings" : {
          "CMAKE_TOOLCHAIN_FILE": "tools_descriptor.cmake",
          "CMAKE_VERBOSE_MAKEFILE" : "ON",
          "FreeRTOS_VERSION": "V10.6.2",
          "FREERTOS_CONFIG_FILE_DIRECTORY" : "${workspaceFolder}/freertos",
          "FREERTOS_PORT" : "GCC_ARM_CM3",
          "DUMP_ASM": "ON"
        } 
      }
    }
  }
}